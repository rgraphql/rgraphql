{
  "name": "soyuz",
  "description": "Fast, minimal, streaming GraphQL client.",
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "peerDependencies": {
    "rgraphql": "^0.5.0",
    "rxjs": "^5.0.0"
  },
  "dependencies": {
    "graphql": "^14.4.2",
    "lodash": "^4.17.0",
    "lru_map": "^0.3.0",
    "rgraphql": "^1.0.3",
    "rxjs": "^6.5.2"
  },
  "scripts": {
    "build": "tsc && ./scripts/build_touchups.bash",
    "test": "npm run lint && npm run mocha",
    "ci": "npm run test && npm run build",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "mocha": "ts-node node_modules/istanbul/lib/cli.js cover -e .ts  -x \"*.d.ts\" -x \"*.spec.ts\" test/run_tests.js",
    "mocha-nocover": "ts-node test/run_tests.js",
    "mocha-nocover-debug": "ts-node debug test/run_tests.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/es6-promise": "^3.0.0",
    "@types/graphql": "^14.2.3",
    "@types/jasmine": "^3.3.16",
    "@types/lodash": "^4.14.136",
    "@types/long": "^4.0.0",
    "@types/node": "^12.6.9",
    "cz-conventional-changelog": "^3.0.2",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine": "^3.0.0",
    "jasmine-console-reporter": "^3.1.0",
    "semantic-release": "^15.13.19",
    "ts-node": "^8.3.0",
    "tslint": "^5.4.2",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rgraphql/soyuz.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "release"
  }
}
