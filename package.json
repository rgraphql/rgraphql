{
  "name": "rgraphql",
  "version": "0.7.1",
  "description": "Common files for the rgraphql system.",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "graphql": "^14.0.0",
    "protobufjs": "^6.0.0"
  },
  "scripts": {
    "build": "tsc && ./scripts/build_touchups.bash",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "ci": " npm run lint && npm run test && npm run build",
    "lint": "tslint -c tslint.json --project tsconfig.json --type-check",
    "gen-proto:go": "./scripts/gen_goproto.bash",
    "gen-proto": "pbjs -t static-module -w commonjs -o src/proto/proto.js ./src/rgraphql.proto && pbts -o src/proto/proto.d.ts src/proto/proto.js",
    "github-release": "conventional-github-releaser -p angular"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/graphql": "^14.0.0",
    "@types/jasmine": "^2.8.0",
    "@types/long": "^4.0.0",
    "@types/node": "^10.5.0",
    "jasmine": "^3.1.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.0.0",
    "typescript": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rgraphql/rgraphql.git"
  }
}
